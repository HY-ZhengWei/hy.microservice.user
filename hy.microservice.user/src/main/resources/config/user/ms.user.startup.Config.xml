<?xml version="1.0" encoding="UTF-8"?>

<config>

    <import name="xconfig"         class="java.util.ArrayList" />
    <import name="startupConfig"   class="java.util.ArrayList" />
    <import name="item"            class="org.hy.common.app.Param" />
    
    
    
    <!-- 启动加载XML配置信息的列表 -->
    <xconfig>
        
        <startupConfig id="StartupConfig_MS_User">
        
            <item if="IsInitDSG_MS_User.getValue == 启用">
                <value>config/user/ms.user.sys.DB.Config.xml</value>
            </item>
            
            <item>
                <value>config/user/ms.user.appKey.Config.xml</value>
            </item>
            
            <item>
                <value>config/user/ms.user.XSSO.xml</value>
            </item>

            <!-- 加载数据库目录所有配置文件 -->
            <item>
                <value>db/user</value>
            </item>
            
        </startupConfig>
        
    </xconfig>
    
</config>